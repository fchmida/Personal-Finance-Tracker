    <li><a href="/users/logout">Logout</a></li>

    <h1 class="ul-header">User List</h1>
    <div class="table-container">
    <table border="1">
      <thead>
        <tr>
          <th>ID</th>
          <th>Username</th>
          <th>Name</th>
          <th>Email</th>
        </tr>
      </thead>
      <tbody>
        <% users.forEach(user => { %>
        <tr>
          <td><%= user.user_id %></td>
          <td><%= user.username %></td>
          <td><%= user.name %></td>
          <td><%= user.email %></td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </div>
  </head>